int enter=0;
Food Helado;
Food Soda;
Food Hamburguesa;
Food Pinia;
Food Manzana;
Food Fresa;

 void setup()
 {
  Helado = new Food("Helado", 854, 78, 1, 0);
  Soda = new Food("Soda", 882, 72, 0, 1);
  Hamburguesa = new Food("Hamburguesa", 913, 64, 0, 2);
  Pinia = new Food("Pinia", 980, 52, 2, 3);
  Manzana = new Food("Manzana", 1095, 46, 2, 4);
  Fresa = new Food("Fresa", 950, 58, 1, 5);
  
  size(1200,700);
  background(255);
 }


  void draw()
  {
  if (enter == 0)
    {//portada
      pantalla1();
      println (p1[0],p1[1],p2[0],p2[1],key, turno);
    }
    
  else if(enter == 1)
    {//selecci√≥n de personajes
      pantalla2();
      println (p1[0],p1[1],p2[0],p2[1],key);
      println (p1[0],p1[1],p2[0],p2[1],key);
      
    }
    
  else if(enter == 2)
    {//juego
      background(0);
      println (p1[0],p1[1],p2[0],p2[1],key, turno);
      pantalla3();
    }
  
  else if (enter == 5)
    {//resultados
      background(0);
      println (p1[0],p1[1],p2[0],p2[1],key, turno);
      pantalla4();
    }
  }

class Food()
{
  String nombre;
  int salud;
  int ataque;
  int clase;
  int pic;
  
  Mounstro(String nombre_, int salud_, int ataque_, int clase_, int pic_)
  {
    nombre = nombre_;
    salud = salud_;
    ataque = ataque_;
    clase = clase_;
    pic = pic_;
  }
}

